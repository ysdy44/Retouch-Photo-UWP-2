<Page
    x:Class="Retouch_Photo2.Menus.Models.DebugMainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignWidth="200"
    d:DesignHeight="400"
    >


    <StackPanel x:Name="StackPanel" MinHeight="165" MaxHeight="300">

        <StackPanel Orientation="Horizontal">
            <Button x:Name="Button" Style="{StaticResource AccentButtonStyle}">
                Layerages
            </Button>
            <Button x:Name="Button2" Style="{StaticResource AccentButtonStyle}">
                Layers
            </Button>
        </StackPanel>

        <StackPanel Orientation="Horizontal">
            <Button x:Name="Coo" Style="{StaticResource AccentButtonStyle}">
                Coo
            </Button>
            <Button x:Name="Re" Style="{StaticResource AccentButtonStyle}">
                Re
            </Button>
        </StackPanel>

        <ItemsControl x:Name="ItemsControl"/>
    </StackPanel>
</Page>

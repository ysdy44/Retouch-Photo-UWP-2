            <Button Grid.Column="1" Content="z" FontSize="20" Margin="1,0" Padding="0" BorderThickness="0" Background="{ThemeResource AppForeground}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
            <Button Grid.Column="2" Content="x" FontSize="20" Margin="1,0" Padding="0" BorderThickness="0" Background="{ThemeResource AppForeground}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>

                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1.5*"/>
            </Grid.ColumnDefinitions>
            <Button Grid.Column="0" Content="?123" FontSize="14" Margin="1,0" Padding="0" BorderThickness="0" Background="{ThemeResource AppStroke}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>

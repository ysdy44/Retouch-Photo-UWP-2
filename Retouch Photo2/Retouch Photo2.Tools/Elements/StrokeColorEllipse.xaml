<UserControl
    x:Class="Retouch_Photo2.Tools.Elements.StrokeColorEllipse"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:brushs="using:Retouch_Photo2.Brushs"
    mc:Ignorable="d"
    d:DesignHeight="50"
    d:DesignWidth="50"
    >

    <UserControl.Resources>
        <Style x:Name="FlyoutPresenterStyle" x:Key="FlyoutPresenterStyleKey" TargetType="FlyoutPresenter" BasedOn="{StaticResource AppFlyoutPresenterStyle}"/>
    </UserControl.Resources>

    <brushs:BrushShowControl x:Name="ColorEllipse" FillOrStroke="Stroke" Fill="{x:Bind SelectionViewModel.Stroke, Mode=OneWay}"/>

</UserControl>